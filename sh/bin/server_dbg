		LOG("authorized".red);
		var connection = new WebSocket(request, socket, head, {});

		// [[ debug
		var isDataSilent = function isDataSilent(data) {
			return String(data).match(/^\{"what":"p[io]ng","p[io]ng":"/);
		};
		connection.on("message", function (message) {
			return !isDataSilent(message) && LOG("<< " + request.url.red + " " + message.cyan);
		});
		connection.on("dbg_send", function (message) {
			return !isDataSilent(message) && LOG(">> " + request.url.red + " " + message.cyan);
		});
		var oldSend = connection.send;
		connection.send = function (message) {
			this.emit("dbg_send", message);
			oldSend.apply(this, arguments);
		};
		connection.on("open", function () {
			return LOG("websocket open".green, request.url.red);
		});
		connection.on("close", function () {
			return LOG("websocket close".green, request.url.red);
		});
		connection.on("error", function (error) {
			return LOG("websocket error".green, request.url.red, error);
		});
		// debug ]]

		connection = PingPongJSON(NodeWebSocketToJSON(connection));
		connection.onerror = function () {
			return connection.close();
		};

		switch (request.url) {
			case "/sh":
				{
					var sub = new SubConnections(connection);
					modem.devices.wsService(sub.open("database"));
					commandConnection(sub.open("command"));
					break;
				}
			// TODO: delete all below later
			case "/devices":
			case "/places":
			case "/database":
				{
					modem.devices.wsService(connection);
					break;
				}
			case "/commands":
				{
					commandConnection(connection);
					break;
				}
			default:
				{
					connection.close();
				}
		}
	});
};

