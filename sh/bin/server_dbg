var waitForSocket = function waitForSocket(socket) {
	return new Promise(function (resolve, reject) {
		socket.on("error", reject);
		socket.on("end", reject);
		socket.on("connect", resolve);
	});
};

var endOnError = function endOnError() {
	this.end();
};

